<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE j8583-config PUBLIC "-//J8583//DTD CONFIG 1.0//EN"
"http://j8583.sourceforge.net/j8583.dtd">
<j8583-config>

    <!-- this is for binary tests (message encoding AND fields) -->
    <parse type="0800">
        <field num="2" type="ALPHA" length="16"/>
        <field num="7" type="DATE10" length="10"/>
        <field num="11" type="NUMERIC" length="6"/>
        <field num="32" type="NUMERIC" length="6"/>
        <field num="70" type="NUMERIC" length="3"/>
    </parse>

    <parse type="0810" extends="0800">
        <field num="39" type="ALPHA" length="2"/>
    </parse>

</j8583-config>
